<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ambow.lyu.modules.eval.dao.StudentEvalTaskDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.ambow.lyu.modules.eval.entity.StudentEvalTaskEntity" id="studentEvalTaskMap">
        <result property="id" column="id"/>
        <result property="taskId" column="task_id"/>
        <result property="percentage" column="percentage"/>
    </resultMap>

    <select id="pageGetStudentList" resultType="com.ambow.lyu.modules.eval.vo.StudentEvalTaskVo">
        SELECT st.id,t.name,t.create_time as createTime,st.percentage,t.dept_id as deptId FROM tb_student_eval_task st left join tb_eval_task t on st.task_id = t.id where t.status = #{evalTaskStatus,jdbcType=TINYINT}
    </select>


</mapper>